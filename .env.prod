# Production Environment Variables
# Copy this to .env.prod and update with your actual values

# Database Configuration
POSTGRES_PASSWORD=your-super-secure-postgres-password-here
DATABASE_URL=postgresql+asyncpg://migrate_user:your-super-secure-postgres-password-here@postgres:5432/migrate_prod

# Redis Configuration
REDIS_URL=redis://redis:6379

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO
ENVIRONMENT=production
SECRET_KEY=your-super-secret-key-change-in-production-make-it-long-and-random

# CORS Configuration
ALLOWED_ORIGINS=["https://migrate-ai.com","https://app.migrate-ai.com","https://api.migrate-ai.com"]

# External APIs (for future use)
OPENAI_API_KEY=your-openai-api-key-here
MCP_SERVER_URL=https://mcp-server.migrate-ai.com

# AWS Configuration (for future deployment)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# Monitoring and Logging
SENTRY_DSN=your-sentry-dsn-here
LOG_GROUP_NAME=/aws/ecs/migrate-backend 